<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="bit.annanma1.thirdpartylibrariesapp">

    <application tools:replace="android:icon"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".RippleBackgroundActivity"
            android:label="@string/ripple_bg" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExplodeImageActivity"
            android:label="@string/title_activity_explode_image" >
        </activity>
        <activity
            android:name=".StandUpActivity"
            android:label="@string/title_activity_stand_up" >
        </activity>
    </application>

</manifest>
